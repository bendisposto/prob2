RULES_MACHINE Replaces
OPERATIONS
COMPUTATION COMP_comp1
BODY
  DEFINE
    foo
  TYPE
    POW(INTEGER)
  VALUE
    {}
  END//DEFINE
END//COMPUTATION

;
COMPUTATION COMP_comp1Replaced
REPLACES COMP_comp1
BODY
  DEFINE
    foo
  TYPE
    POW(INTEGER)
  VALUE
    {1}
  END//DEFINE
END//COMPUTATION

;
RULE RULE_Rule1
RULEID id1
ERROR_TYPES 1
BODY
  RULE_FAIL
    x
  WHEN
    x : foo
  COUNTEREXAMPLE
    STRING_FORMAT("value ~w", x)
  END//RULE_FAIL
END


END
