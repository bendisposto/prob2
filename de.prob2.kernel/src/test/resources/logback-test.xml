<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d, T+%relative] "%thread" %logger.%method\(%file:%line\): [%level] %msg%n%ex{full}</pattern>
		</encoder>
	</appender>
	
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
